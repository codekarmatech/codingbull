# Hostinger VPS Production Environment Configuration
# Enterprise-grade configuration with zero external dependencies
# Generated: 2025-06-27

NODE_ENV=production
REACT_APP_ENV=production

# API Configuration - HOSTINGER VPS NATIVE
# Direct connection to local Hostinger VPS backend
REACT_APP_API_BASE_URL=https://api.codingbullz.com/api/v1
REACT_APP_API_TIMEOUT=10000
REACT_APP_API_RETRY_ATTEMPTS=3
REACT_APP_API_RETRY_DELAY=1000
REACT_APP_API_HEALTH_CHECK_INTERVAL=30000

# Security Configuration - ENTERPRISE GRADE
REACT_APP_CSP_NONCE=""
REACT_APP_ALLOWED_ORIGINS=https://codingbullz.com,https://www.codingbullz.com
REACT_APP_ENABLE_STRICT_CSP=true
REACT_APP_ENABLE_SECURITY_HEADERS=true
REACT_APP_ENABLE_CONTENT_SECURITY_POLICY=true
REACT_APP_ENABLE_XSS_PROTECTION=true

# Analytics & Monitoring - SELF-HOSTED ONLY
# No external analytics services for enterprise compliance
REACT_APP_GOOGLE_ANALYTICS_ID=""
REACT_APP_SENTRY_DSN=""
REACT_APP_HOTJAR_ID=""
REACT_APP_ENABLE_SELF_HOSTED_ANALYTICS=true
REACT_APP_ENABLE_LOCAL_METRICS=true

# Feature Flags - HOSTINGER VPS PRODUCTION
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_DEBUG_MODE=false
REACT_APP_ENABLE_REDUX_DEVTOOLS=false
REACT_APP_MOCK_API=false
REACT_APP_ENABLE_ENTERPRISE_FEATURES=true
REACT_APP_ENABLE_OFFLINE_SUPPORT=true

# Contact & Support
REACT_APP_SUPPORT_EMAIL=support@codingbullz.com
REACT_APP_CONTACT_EMAIL=contact@codingbullz.com

# Social Media Links
REACT_APP_GITHUB_URL=https://github.com/CodingBullTechnovations
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/codingbull
REACT_APP_TWITTER_URL=https://twitter.com/codingbull

# Build Configuration - HOSTINGER VPS OPTIMIZED
REACT_APP_BUILD_VERSION=1.0.0
REACT_APP_BUILD_DATE=2025-06-27
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
REACT_APP_BUILD_TARGET=hostinger-vps
REACT_APP_DEPLOYMENT_PLATFORM=hostinger-vps

# Performance Configuration - VPS OPTIMIZED
REACT_APP_LAZY_LOADING_THRESHOLD=0.1
REACT_APP_IMAGE_OPTIMIZATION=true
REACT_APP_CACHE_DURATION=3600
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_ENABLE_COMPRESSION=true
REACT_APP_ENABLE_PRELOAD_CRITICAL_RESOURCES=true
REACT_APP_ENABLE_RESOURCE_HINTS=true

# Hostinger VPS Native Optimization
REACT_APP_ENABLE_NGINX_CACHING=true
REACT_APP_STATIC_ASSET_CDN=false
REACT_APP_ENABLE_GZIP_COMPRESSION=true
REACT_APP_ENABLE_BROTLI_COMPRESSION=true
REACT_APP_ENABLE_HTTP2_PUSH=true
REACT_APP_ENABLE_CRITICAL_CSS_INLINING=true

# Production Settings - ZERO EXTERNAL DEPENDENCIES
REACT_APP_ENABLE_MOCK_FALLBACK=false
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_DEPLOYMENT_TARGET=hostinger-vps
REACT_APP_HOSTING_PROVIDER=hostinger
REACT_APP_INFRASTRUCTURE_TYPE=vps

# Development Tools - COMPLETELY DISABLED
REACT_APP_ENABLE_MOCK_FALLBACK=false
REACT_APP_ENABLE_HOT_RELOAD=false
REACT_APP_ENABLE_REACT_DEVTOOLS=false
REACT_APP_ENABLE_WEBPACK_BUNDLE_ANALYZER=false

# Enterprise Security Features
REACT_APP_ENABLE_CONTENT_SECURITY_POLICY=true
REACT_APP_ENABLE_SUBRESOURCE_INTEGRITY=true
REACT_APP_ENABLE_REFERRER_POLICY=true
REACT_APP_ENABLE_FEATURE_POLICY=true

# Hostinger VPS Specific Settings
REACT_APP_VPS_REGION=auto
REACT_APP_ENABLE_LOCAL_STORAGE_ENCRYPTION=true
REACT_APP_ENABLE_SESSION_SECURITY=true
REACT_APP_ENABLE_CSRF_PROTECTION=true
